<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <title>Voting Blocks</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  <link rel="stylesheet" href="css/styles.css">
  <script type="text/javascript">
      window.history.forward();
      function noBack() {
          window.history.forward();
      }
  </script>
</head>

<body>
  <div class="box" id="heading">
    <h1> Vote Chain </h1>
  </div>

  <div class="container mx-auto mt-4">

    <div class="row">
            <%for(var i=0;i<votingblocks.length;i++){%>
      <div class="col-md-6">
        <div class="card" style="width: 18rem;">
          <div class="card-body">
            <h5 class="card-title">Block: <%= i %></h5>
            <!-- <h6 class="card-subtitle mb-2 text-muted">Card subtitle</h6> -->
            <!-- <p class="card-text">timestamp: <%= votingblocks[i].timestamp%></p>
            <p class="card-text">LastHash:  <%= votingblocks[i].lastHash%></p>
            <p class="card-text">Hash: <%= votingblocks[i].hash%></p>
            <p class="card-text">Data: <%= votingblocks[i].data%></p>
            <p class="card-text">Nonce:<%= votingblocks[i].nonce%></p> -->



            <table class="table table-striped table-dark">

            <tbody>
              <tr>
                <th scope="row">Timestamp</th>
                <td><%= votingblocks[i].timestamp%></td>
              </tr>
              <tr>
                <th scope="row">LastHash</th>
                <td><%= votingblocks[i].lastHash%></td>

              </tr>
              <tr>
                <th scope="row">Hash</th>
                <td><%= votingblocks[i].hash%></td>

              </tr
              <tr>
                <th scope="row">Data</th>
                <td><%= votingblocks[i].data%></td>

              </tr>
              <tr>
                <th scope="row">Nonce</th>
                <td><%= votingblocks[i].nonce%></td>

              </tr>
            </tbody>
          </table>

          </div>
        </div>
      </div>
        <% } %>
    </div>

  </div>
  <form class="" action="/" method="get">
    <button class="btn btn-primary" type="submit">Logout</button>
  </form>

</body>

</html>
